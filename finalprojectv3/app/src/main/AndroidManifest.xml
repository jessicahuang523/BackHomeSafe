<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="club.owo.finalproject_v3">

    <uses-permission android:name="android.permission.CAMERA" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/bhs_logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.Finalprojectv3">
        <activity android:name="club.owo.finalproject_v3.ScannerActivity"></activity>
        <activity
            android:name="club.owo.finalproject_v3.MainActivity"
            android:label="@string/app_name" />
        <activity android:name="club.owo.finalproject_v3.ShopRegisterActivity" />
        <activity android:name="club.owo.finalproject_v3.SignUpActivity" />
        <activity android:name="club.owo.finalproject_v3.SignInActivity" />
        <activity android:name="club.owo.finalproject_v3.QRCodeActivity" />
        <activity android:name="club.owo.finalproject_v3.WelcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>